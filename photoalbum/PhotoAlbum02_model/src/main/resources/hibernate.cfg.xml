<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory name="factory">
		<property name="connection.datasource">java:comp/env/jdbc/PhotoAlbum</property>
		<property name="hibernate.show_sql">true</property>
		<property name="hibernate.use_sql_comments">true</property>
		<property name="hibernate.dialect">org.hibernate.dialect.H2Dialect</property>
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.User" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.Album" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.File" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.ShareInformationAlbums" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.ShareInformationPhotos" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.Tag" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.TagAlbum" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.TagFile" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.Comment" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.CommentAlbum" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.CommentFile" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.Likefield" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.LikeAlbum" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.LikeFile" />
		<mapping class="es.udc.fi.dc.photoalbum.hibernate.LikeComment" />
	</session-factory>
</hibernate-configuration>
